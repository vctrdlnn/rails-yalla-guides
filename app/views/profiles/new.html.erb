<h1>Profiles#new</h1>
<p>Find me in app/views/profiles/new.html.erb</p>

<%= simple_form_for @profile do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :role, collection: ["host", "guest"], selected: "guest" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
